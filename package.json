{
  "name": "stm",
  "version": "0.9.0",
  "description": "Solace Try-Me Command Line Tool",
  "keywords": [
    "solace",
    "tryme",
    "try-me",
    "pub/sub",
    "publish/subscribe",
    "publish",
    "subscribe"
  ],
  "scripts": {
    "dev": "tsc -w",
    "build": "tsc"
  },
  "main": "./dist/src/index.js",
  "bin": {
    "stm": "./bin/index.js"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@types/long": "^5.0.0",
    "chalk": "~4.1.2",
    "commander": "^9.3.0",
    "concat-stream": "^2.0.0",
    "core-js": "^3.26.0",
    "quoted-string-space-split": "^1.1.1",
    "signale": "^1.4.0",
    "solclientjs": "^10.15.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/concat-stream": "^2.0.0",
    "@types/node": "^17.0.43",
    "@types/signale": "^1.4.4",
    "typescript": "^4.7.3"
  },
  "pkg": {
    "targets": [
      "node16-linux-x64",
      "node16-macos-x64",
      "node16-win-x64",
      "node16-alpine-x64",
      "node16-linux-arm64",
      "node16-macos-arm64",
      "node16-win-arm64",
      "node16-alpine-arm64"
    ],
    "outputPath": "release",
    "scripts": [
      "dist/src/scenarios/**/*",
      "node_modules/solclientjs/**/*.js"
    ]
  }
}
