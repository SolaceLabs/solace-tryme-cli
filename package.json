{
  "name": "stm",
  "version": "0.0.1",
  "description": "Solace Try-Me Command Line Tool",
  "keywords": [
    "solace",
    "tryme",
    "try-me",
    "pub/sub",
    "pub/recv",
    "publish/subscribe",
    "publish/receive",
    "publish",
    "subscribe"
  ],
  "scripts": {
    "dev": "tsc -w",
    "build": "tsc"
  },
  "main": "./dist/src/index.js",
  "bin": {
    "stm": "./bin/index.js"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@base2/pretty-print-object": "^1.0.2",
    "@types/long": "^5.0.0",
    "chalk": "~4.1.2",
    "commander": "^9.3.0",
    "concat-stream": "^2.0.0",
    "core-js": "^3.26.0",
    "prompt-sync": "^4.2.0",
    "signale": "^1.4.0",
    "solclientjs": "^10.15.0"
  },
  "devDependencies": {
    "@types/concat-stream": "^2.0.0",
    "@types/node": "^17.0.43",
    "@types/signale": "^1.4.4",
    "typescript": "^4.7.3"
  },
  "pkg": {
    "targets": [
      "node18-linux-x64",
      "node18-macos-x64",
      "node18-win-x64",
      "node18-alpine-x64",
      "node18-linux-arm64",
      "node18-macos-arm64",
      "node18-win-arm64",
      "node18-alpine-arm64"
    ],
    "outputPath": "release",
    "scripts": [
      "node_modules/solclientjs/**/*.js"
    ]
  }
}
