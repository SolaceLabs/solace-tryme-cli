{
  "name": "stm",
  "version": "0.0.25",
  "description": "Solace Try-Me Command Line Tool",
  "keywords": [
    "solace",
    "tryme",
    "try-me",
    "pub/sub",
    "pub/recv",
    "publish/subscribe",
    "publish/receive",
    "publish",
    "subscribe"
  ],
  "scripts": {
    "dev": "tsc -w",
    "index": "ts-node bin/index.js",
    "build": "tsc",
    "package": "pkg package.json"
  },
  "main": "./dist/src/index.js",
  "bin": {
    "stm": "./bin/index.js"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@base2/pretty-print-object": "^1.0.2",
    "@types/long": "^5.0.0",
    "@types/node": "^20.8.9",
    "ascii-table": "^0.0.9",
    "chalk": "~4.1.2",
    "commander": "^11.1.0",
    "concat-stream": "^2.0.0",
    "core-js": "^3.26.0",
    "express": "^4.18.2",
    "live-server": "^1.2.2",
    "node-localstorage": "^3.0.5",
    "opener": "^1.5.2",
    "pkg": "^5.8.1",
    "prompt-sync": "^4.2.0",
    "signale": "^1.4.0",
    "solclientjs": "^10.15.0",
    "ts-node": "^10.9.1",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@types/concat-stream": "^2.0.0",
    "@types/signale": "^1.4.4",
    "typescript": "^4.7.3"
  },
  "signale": {
    "displayLabel": false,
    "displayDate": false,
    "displayTimestamp": false
  },
  "pkg": {
    "targets": [
      "node18-linux-x64",
      "node18-macos-x64",
      "node18-win-x64",
      "node18-alpine-x64"
    ],
    "outputPath": "release",
    "assets": [
      "node_modules/solclientjs/**/*.js",
      "public/**/*"
    ]
  }
}
